html,
body {
	height: 100%;
	box-sizing: border-box;
	background-color: #d6514e;
	padding: 0;
	margin: 0;
}

*,
*::before,
*::after {
	box-sizing: inherit;
}

a {
	color: inherit;
	text-decoration: none;
}

li {
	list-style: none;
}


.gulp {
	width: 100vw;
	height: 100vh;
	background-image: url('../assets/gulp-cover.png');
	background-position: center;
	background-size: contain;
	background-repeat: no-repeat;
}